<div>
  <% review.comments.each do |comment| %>
    <p><%= image_tag comment.end_user.get_profile_image(100,100) %></p>
    <%= comment.end_user.nick_name %>
    <%= comment.created_at.strftime('%Y/%m/%d') %><%= comment.comment %>
    <% if comment.end_user == current_end_user %>
      <%= link_to "削除", review_comment_path(comment.review, comment), method: :delete, remote:true %>
    <% end %>
  <% end %>
</div>